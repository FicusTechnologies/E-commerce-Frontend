    "eslint-plugin-vue": "^9.19.2",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "nuxt": "^3.8.2",
    "nuxt-vitest": "^0.11.5",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.5",
    "storybook": "^7.6.10",
    "typescript": "^5.3.3",
    "vitest": "^0.33.0",
    "vue": "^3.3.12",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@storybook/addon-postcss": "^2.0.0",
